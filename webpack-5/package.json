{
  "name": "devtool-webpack",
  "version": "1.0.0",
  "description": "A tooling for web development and prototyping using webpack.",
  "author": "Lau Tiam Kok",
  "private": true,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "webpack serve --mode development --output-public-path='/'",
    "watch": "webpack --mode development --watch --output-public-path='/wp-content/themes/theme-xyz/'",
    "build": "webpack --mode production --output-public-path='/'",
    "build:gh-pages": "webpack --mode production --output-public-path='/devtool-webpack/'",
    "purge": "purgecss --config ./purgecss.config.js --output dist/assets/css/",
    "deploy": "push-dir --dir=dist --branch=gh-pages --cleanup"
  },
  "dependencies": {
    "@babel/runtime": "^7.11.2",
    "axios": "^0.21.1",
    "mitt": "^2.1.0",
    "vue": "^2.6.12",
    "vue-router": "^3.4.3",
    "vue-router-multiguard": "^1.0.3",
    "vuex": "^3.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "autoprefixer": "^10.2.6",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.2.1",
    "css-loader": "^5.0.0",
    "css-minimizer-webpack-plugin": "^1.1.5",
    "file-loader": "^6.1.1",
    "html-loader": "^1.3.2",
    "html-webpack-plugin": "^5.0.0",
    "less": "^3.12.2",
    "less-loader": "^7.0.2",
    "mini-css-extract-plugin": "^1.1.0",
