    "purgecss": "^4.0.3",
    "push-dir": "^0.4.1",
    "sass-loader": "^10.0.3",
    "style-loader": "^2.0.0",
    "tailwindcss": "^2.2.4",
    "terser-webpack-plugin": "^5.0.0",
    "url-loader": "^4.1.1",
    "vue-loader": "^15.9.3",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^5.1.3",
    "webpack-bundle-analyzer": "^4.4.1",
    "webpack-cli": "^4.1.0",
    "webpack-dev-server": "^3.11.0",
