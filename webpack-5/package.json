{
  "name": "devtool-webpack",
  "version": "1.0.0",
  "description": "A setup for frontend web development and prototyping using webpack, Vue.js, and Tailwind CSS.",
  "author": "Lau Tiam Kok",
  "private": true,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "webpack serve --mode development --output-public-path='/'",
    "watch": "webpack --mode development --watch --output-public-path='/wp-content/themes/theme-xyz/'",
    "build": "webpack --mode production --output-public-path='/'",
    "build:gh-pages": "webpack --mode production --output-public-path='/devtool-webpack/'",
    "purge": "purgecss --config ./purgecss.config.js --output dist/assets/css/",
    "deploy": "push-dir --dir=dist --branch=gh-pages --cleanup"
  },
  "dependencies": {
    "@babel/runtime": "^7.14.6",
    "@vue/compiler-sfc": "^3.1.3",
    "axios": "^0.21.1",
    "mitt": "^3.0.0",
    "vue": "^2.6.14",
    "vue-router": "^3.5.2",
    "vue-router-multiguard": "^1.0.3",
    "vuex": "^3.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "^7.14.7",
    "autoprefixer": "^10.2.6",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "copy-webpack-plugin": "^9.0.1",
    "css-loader": "^5.2.6",
    "css-minimizer-webpack-plugin": "^3.0.2",
    "file-loader": "^6.2.0",
    "html-loader": "^1.3.2",
    "html-webpack-plugin": "^5.3.2",
    "less": "^4.1.1",
    "less-loader": "^10.0.0",
