    "postcss-loader": "^6.1.1",
    "purgecss": "^4.0.3",
    "push-dir": "^0.4.1",
    "sass-loader": "^10.2.0",
    "style-loader": "^2.0.0",
    "tailwindcss": "^2.2.4",
    "terser-webpack-plugin": "^5.1.4",
    "url-loader": "^4.1.1",
    "vue-loader": "^16.3.0",
    "vue-style-loader": "^4.1.3",
    "vue-template-compiler": "^2.6.14",
    "webpack": "^5.42.0",
    "webpack-cli": "^4.7.2",
